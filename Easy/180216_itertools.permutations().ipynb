    "given = input().split()\n",
    "word = given[0]\n",
    "number = int(given[1])\n",
    "\n",
    "ls = list(permutations(word, number))\n",
    "ls = [\"\".join(i) for i in ls]   \n",
    "ls.sort()\n",
    "\n",
    "for i in ls :\n",
    "    print(i)"
